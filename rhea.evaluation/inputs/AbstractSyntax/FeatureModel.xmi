<?xml version="1.0" encoding="UTF-8"?>
<rhea:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:rhea="rhea.metamodels.BasicFMs"
    xmlns:rhea_1="rhea.metamodels.NumericalFMs"
    xmlns:rhea_2="rhea.metamodels.PropLogicCTCs"
    xmlns:rhea_3="rhea.metamodels.ComparativeCTCs"
    xsi:schemaLocation="rhea.metamodels.BasicFMs ../../../rhea.metamodels/metamodels/BasicFMs/BasicFMs.ecore rhea.metamodels.NumericalFMs ../../../rhea.metamodels/metamodels/NumericalFMs/NumericalFMs.ecore rhea.metamodels.PropLogicCTCs ../../../rhea.metamodels/metamodels/PropLogicCTCs/PropLogicCTCs.ecore rhea.metamodels.ComparativeCTCs ../../../rhea.metamodels/metamodels/ComparativeCTCs/ComparativeCTCs.ecore">
  <root id="Root"
      name="Root">
    <children
        xsi:type="rhea_1:NumericalFeature"
        id="N1"
        name="N1"
        mandatory="true"
        value="2.0"/>
  </root>
  <crosstreeconstraints
      xsi:type="rhea_2:AdvancedConstraint">
    <expr xsi:type="rhea_2:And">
      <terms
          xsi:type="rhea_3:Less">
        <left
            xsi:type="rhea_2:FeatureTerm"
            feature="//@root/@children.0"/>
        <right
            xsi:type="rhea_3:NumericTerm"
            value="8.0"/>
      </terms>
    </expr>
  </crosstreeconstraints>
</rhea:FeatureModel>
